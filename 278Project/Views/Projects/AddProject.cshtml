@{
    ViewData["Title"] = "AddProject";
}

@model ProjectModel
<form action="/Projects/AddProject" method="post">
  <div class="form-group">
    <label for="Name">Name</label>
    <input type="text" class="form-control" id="Name" asp-for="Name">
  </div>
  <div class="form-group">
    <label for="Description">Description</label>
    <input type="text" class="form-control" id="Description" asp-for="Desription">
  </div>
  <div class="form-group">
    <label for="Status">Status</label>
    <input type="text" class="form-control" id="Status" placeholder="Assigned or UnAssigned" asp-for="Status">
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
